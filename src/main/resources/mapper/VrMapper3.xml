<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.znsm.znsm.mapper.VrMapper3">

    <insert id="addvr3" parameterType="Vr">
        insert into vr3 (x,y,a,x1,y1,z,a1,b,c) values (#{x},#{y},#{a},#{x1},#{y1},#{z},#{a1},#{b},#{c})
    </insert>

</mapper>
