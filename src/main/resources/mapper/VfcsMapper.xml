<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.znsm.znsm.mapper.VfcsMapper">

    <insert id="addindex" parameterType="Vfcs">
        insert into vfcs (x,y,z,a,b,c,x1,y1) values (#{x},#{y},#{z},#{a},#{b},#{c},#{x1},#{y1})
    </insert>

    <select id="getAllindex" resultType="Vfcs">
      select * from vfcs;
    </select>
</mapper>
