<mapper namespace="com.znsm.znsm.mapper.PrcovrcMapper1">

    <insert id="addprcovrc1" parameterType="Prcovrc">
        insert into prcovrc (x,y,a,j1,j2,j3,j4,j5,j6,j7,j8) values (#{x},#{y},#{a},#{j1},#{j2},#{j3},#{j4},#{j5},#{j6},#{j7},#{j8})
