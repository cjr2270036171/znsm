<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.znsm.znsm.mapper.IndexMapper">

    <select id="index" parameterType="Ztjm" resultType="Ztjm">
      select * from ztjm where ipaddress=#{ipaddress} and dkh=#{dkh} and cmd=#{cmd};
    </select>

    <select id="cs" resultType="Ztjm">
      select * from ztjm;
    </select>

</mapper>
